    "version": "3.0.2",
    "author": {
        "name": "Frederik Braun et al."
    },
    "bugs": {
        "url": "https://github.com/mozilla/eslint-plugin-no-unsanitized/issues"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.3",
