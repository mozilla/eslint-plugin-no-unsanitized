        "mocha": "^5.1.1"
    },
    "peerDependencies": {
        "eslint": ">=3"
    },
    "homepage": "https://github.com/mozilla/eslint-plugin-no-unsanitized/",
    "keywords": [
        "eslint",
        "eslint-plugin",
        "eslintplugin",
        "lint",
        "sanitize",
        "innerHTML",
        "security"
    ],
    "license": "MPL-2.0",
    "main": "index.js",
    "repository": {
        "type": "git",
